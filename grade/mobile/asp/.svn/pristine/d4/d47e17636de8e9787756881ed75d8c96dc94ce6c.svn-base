<script type="text/javascript">
    apiready = function(){
        var oReturn = $api.dom('.ui-icon-return');
        var oLi = $api.domAll('.wyt-tab li');
        var aUl = $api.domAll('.wyt-list');
        $api.addEvt(oReturn, 'click', function () {
            close();
        });

        for (var i=0; i<oLi.length;i++) {
            oLi[i].index = i;
            $api.addEvt(oLi[i], 'click', function () {
                change(this);
            });
        }

        function close () {
            api.closeFrame({
                name: 'mypay'
            });
        }

        function change (obj) {
            var oIndex = obj.index;
            for (var i=0;i<oLi.length;i++) {
                if (i!=oIndex) {
                    $api.removeCls(oLi[i], 'active');
                    $api.css(aUl[i], 'display:none');
                }
            }
            $api.addCls(obj, 'active');
            $api.css(aUl[oIndex], 'display:block');
        }

    }
</script>
