<script type="text/javascript">
	apiready = function(){
	    var oIndex = 0;
        var oAdd = $api.byId('add'); 
        var oPayment = $api.byId('payment');  
	    $('.ui-searchbar').tap(function(){
	        $('.ui-searchbar-wrap').addClass('focus');
	        $('.ui-searchbar-input input').focus();
	    });
	    $('.ui-searchbar-cancel').tap(function(){
	        $('.ui-searchbar-wrap').removeClass('focus');
	    });

	    (function() {
	        var tab = new fz.Scroll('.ui-tab', {
	            role: 'tab',
	        });

	        tab.on('beforeScrollStart', function(from, to) {
	            console.log(from, to);
	        });

	        tab.on('scrollEnd', function(curPage) {
	            console.log(curPage);
	        });
	    })();

        $api.addEvt(oAdd, 'click', function () {
            api.openFrame({
                name: 'newbill',
                url: './waybill/newbill.html',
                bounces: false,
                rect: {
                    x: 0,
                    y: 0,
                    w: 'auto',
                    h: 'auto',
                    marginBottom: 56
                }
            });
        });

        $api.addEvt(oPayment, 'click', function () {
            api.openFrame({
                name: 'mypay',
                url: './mine/mypay.html',
                bounces: false,
                rect: {
                    x: 0,
                    y: 0,
                    w: 'auto',
                    h: 'auto',
                    marginBottom: 56
                }
            });
        });
	} 


